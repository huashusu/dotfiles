{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{.Shell}} in {{.PWD}}",
  "blocks": [
     {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ef5350",
          "style": "diamond",
          "template": "Administrator <darkGray>&</> ",
          "type": "root"
        },
        {
          "style": "plain",
          "template": "<cyan>{{ .UserName }}</>",
          "type": "session"
        },
        {
          "foreground": "#21c7c7",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": " <darkGray>in</> <#21c7c7>{{ .Path }}</> ",
          "type": "path"
        },
        {
          "foreground": "#FFE700",
          "foreground_templates": [
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#16c60c{{ end }}",
            "{{ if gt .Behind 0 }}#f450de{{ end }}"
          ],
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "foreground": "#3C873A",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a>\ue5fa</> ",
            "yarn_icon": " <#348cba>\ue6a7</> ",
            "pnpm_icon":" <#348cba>\uF02C1</> "
          },
          "style": "plain",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "type": "node"
        },
        {
          "foreground": "#06aad5",
          "style": "plain",
          "template": "\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "type": "go"
        },
        {
          "foreground": "#925837",
          "style": "plain",
          "template": "\ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "type": "rust"
        },
        {
          "foreground": "#83769c",
          "properties": {
            "style": "roundrock",
            "threshold": 300
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        },
          {
          "foreground": "#55ffff",
          "properties": {
            "time_format": "2006/01/02 15:04"
          },
          "style": "plain",
          "template": "<#ffffff>[</>{{ .CurrentDate | date .Format }}<#ffffff>]</>",
          "type": "time"
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#e0f8ff",
          "foreground_templates": ["{{ if gt .Code 0 }}#ef5350{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ue285 ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
